{
  "progName": "Alertify Demo",
  "htmlText": "<h3>Demo of using the Alertify.js library</h3>\n\n<button onclick=\"AlertDefault()\">Alert Dialog</button>\n\n<button onclick=\"AlertConfirm()\">Confirm Dialog</button>\n\n<button onclick=\"AlertPrompt()\">Prompt Dialog</button>\n\n<br/><br/>\n\n<button onclick=\"NotifyLog()\">Log Notification</button>\n\n<button onclick=\"NotifySuccess()\">Success Notification</button>\n\n<button onclick=\"NotifyError()\">Error Notification</button>",
  "scriptText": "function AlertDefault()\n{\n\talertify.alert(\"This is a default alert dialog which the user is required to dismiss\");\n}\n\nfunction AlertConfirm() {\n\t// confirm dialog\n\talertify.confirm(\"This is an alert that the user must choose to accept or cancel\", function (e) {\n    \t\tif (e) {\n        \t\t// user clicked \"ok\"\n              \talertify.log(\"ok\");\n    \t\t} else {\n        \t\t// user clicked \"cancel\"\n\t\t\t\talertify.log(\"cancel\");\n            }\n\t});\n}\n\nfunction AlertPrompt() {\n\t// prompt dialog\n\talertify.prompt(\"This is a prompt for the user to enter a string\", function (e, str) {\n    \t\t// str is the input text\n    \t\tif (e) {\n        \t\t// user clicked \"ok\"\n                alertify.log(str);\n    \t\t} else {\n        \t\t// user clicked \"cancel\"\n                alertify.log(\"cancel\");\n    \t\t}\n\t}, \"Default Value\");\n}\n\nfunction NotifyLog() {\n// standard notification\n// setting the wait property to 0 will\n// keep the log message until it's clicked\n//alertify.log(\"Notification\", type, wait);\n\talertify.log(\"This is a Log notification\");\n}\n\nfunction NotifySuccess() {\n// success notification\n// shorthand for alertify.log(\"Notification\", \"success\");\n\talertify.success(\"Success notification\");\n}\n\nfunction NotifyError() {\n// error notification\n// shorthand for alertify.log(\"Notification\", \"error\");\n\talertify.error(\"Error notification\");\n}"
}