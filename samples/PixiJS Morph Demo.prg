  "scriptText": "\tvar w = 1024;\n\tvar h = 768;\n\n\tvar n = 2000;\n\tvar d = 1;\n\tvar current = 1;\n\tvar objs = 17;\n\tvar vx = 0;\n\tvar vy = 0;\n\tvar vz = 0;\n\nvar sbv = {\n\tstopAnim : false,\n\tpoints1 : [],\n\tpoints2 : [],\n\tpoints3 : [],\n\ttpoint1 : [],\n\ttpoint2 : [],\n\ttpoint3 : [],\n    balls : []\n}\n\nfunction EVT_CleanSandbox() {\n  sbv.stopAnim = true;\t// not going to delete this because of a long timer needing it to stop\n  \n  setTimeout(function() {\n\tdelete sbv.points1;\n\tdelete sbv.points2;\n\tdelete sbv.points3;\n    delete sbv.tpoint1;\n    delete sbv.tpoint2;\n    delete sbv.tpoint3;\n\tdelete sbv.balls;\n    sbv = null;\n    EVT_CleanSandbox = null;\n  }, 300);\n}\n\n\nAPI_SetBackgroundColor(\"#333\");\nstart();\n\n\tfunction start() {\n\n\t\tvar ballTexture = new PIXI.Texture.fromImage(\"libraries/pixi.js/pixel.png\");\n\n\t\trenderer = PIXI.autoDetectRenderer(w, h);\n\n\t\tstage = new PIXI.Stage;\n\n\t\t//document.body.appendChild(renderer.view);\n\t\tdocument.getElementById(\"UI_MainPlaceholder\").appendChild(renderer.view);\n\n\t\tmakeObject(0);\n\n\t\tfor (var i = 0; i < n; i++)\n\t\t{\n\t\t\tsbv.tpoint1[i] = sbv.points1[i];\n\t\t\tsbv.tpoint2[i] = sbv.points2[i];\n\t\t\tsbv.tpoint3[i] = sbv.points3[i];\n\n\t\t\tvar tempBall = new PIXI.Sprite(ballTexture);\n\t\t\ttempBall.anchor.x = 0.5;\n\t\t\ttempBall.anchor.y = 0.5;\n\t\t\ttempBall.alpha = 0.5;\n\t\t\tsbv.balls[i] = tempBall;\n\n\t\t\tstage.addChild(tempBall);\n\t\t}\n\n\t\tresize();\n\n\t\tsetTimeout(nextObject, 5000);\n\n\t\trequestAnimFrame(update);\n\n\t}\n\n\tfunction nextObject () {\n\t\tif (sbv.stopAnim) return;\n\t\tcurrent++;\n\n\t\tif (current > objs)\n\t\t{\n\t\t\tcurrent = 0;\n\t\t}\n\n\t\tmakeObject(current);\n\n\t\tsetTimeout(nextObject, 8000);\n\n\t}\n\n\tfunction makeObject ( t ) {\n\n\t\tvar xd;\n\n\t\tswitch (t)\n\t\t{\n\t\t\tcase 0:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\tsbv.points1[i] = -50 + Math.round(Math.random() * 100);\n\t\t\t\t\tsbv.points2[i] = 0;\n\t\t\t\t\tsbv.points3[i] = 0;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 1:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(t * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(xd) * 10) * (Math.sin(t * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(xd) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 2:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(t * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(xd) * 10) * (Math.sin(t * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 3:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(xd) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(xd) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(xd) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 4:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(xd) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(xd) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 5:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(xd) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(i * 360 / n) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 6:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(i * 360 / n) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(i * 360 / n) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 7:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(i * 360 / n) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(i * 360 / n) * 10) * (Math.sin(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 8:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(i * 360 / n) * 10) * (Math.sin(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(xd) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 9:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(i * 360 / n) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(xd) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 10:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(i * 360 / n) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(xd) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 11:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.sin(xd) * 10) * (Math.sin(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(xd) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 12:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(xd) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.sin(xd) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 13:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.sin(i * 360 / n) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 14:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.sin(xd) * 10) * (Math.cos(xd) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.sin(xd) * 10) * (Math.sin(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 15:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(i * 360 / n) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.sin(i * 360 / n) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 16:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.sin(i * 360 / n) * 10) * (Math.sin(xd) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(xd) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\n\t\t\tcase 17:\n\n\t\t\t\tfor (var i = 0; i < n; i++)\n\t\t\t\t{\n\t\t\t\t\txd = -90 + Math.round(Math.random() * 180);\n\t\t\t\t\tsbv.points1[i] = (Math.cos(xd) * 10) * (Math.cos(xd) * 10);\n\t\t\t\t\tsbv.points2[i] = (Math.cos(i * 360 / n) * 10) * (Math.sin(i * 360 / n) * 10);\n\t\t\t\t\tsbv.points3[i] = Math.sin(i * 360 / n) * 100;\n\t\t\t\t}\n\t\t\t\tbreak;\n\t\t}\n\n\t}\n\n\tfunction resize()\n\t{\n\t\tw = 600; //$(window).width() - 16;\n\t\th = 400; //$(window).height() - 16;\n\n\t\trenderer.resize(w, h);\n\t}\n\n\tfunction update()\n\t{\n    \tif (sbv.stopAnim) return;\n        \n\t\tvar x3d, y3d, z3d, tx, ty, tz, ox;\n\n\t\tif (d < 250)\n\t\t{\n\t\t\td++;\n\t\t}\n\n\t\tvx += 0.0075;\n\t\tvy += 0.0075;\n\t\tvz += 0.0075;\n\n\t\tfor (var i = 0; i < n; i++)\n\t\t{\n\t\t\tif (sbv.points1[i] > sbv.tpoint1[i]) { sbv.tpoint1[i] = sbv.tpoint1[i] + 1; }\n\t\t\tif (sbv.points1[i] < sbv.tpoint1[i]) { sbv.tpoint1[i] = sbv.tpoint1[i] - 1; }\n\t\t\tif (sbv.points2[i] > sbv.tpoint2[i]) { sbv.tpoint2[i] = sbv.tpoint2[i] + 1; }\n\t\t\tif (sbv.points2[i] < sbv.tpoint2[i]) { sbv.tpoint2[i] = sbv.tpoint2[i] - 1; }\n\t\t\tif (sbv.points3[i] > sbv.tpoint3[i]) { sbv.tpoint3[i] = sbv.tpoint3[i] + 1; }\n\t\t\tif (sbv.points3[i] < sbv.tpoint3[i]) { sbv.tpoint3[i] = sbv.tpoint3[i] - 1; }\n\n\t\t\tx3d = sbv.tpoint1[i];\n\t\t\ty3d = sbv.tpoint2[i];\n\t\t\tz3d = sbv.tpoint3[i];\n\n\t\t\tty = (y3d * Math.cos(vx)) - (z3d * Math.sin(vx));\n\t\t\ttz = (y3d * Math.sin(vx)) + (z3d * Math.cos(vx));\n\t\t\ttx = (x3d * Math.cos(vy)) - (tz * Math.sin(vy));\n\t\t\ttz = (x3d * Math.sin(vy)) + (tz * Math.cos(vy));\n\t\t\tox = tx;\n\t\t\ttx = (tx * Math.cos(vz)) - (ty * Math.sin(vz));\n\t\t\tty = (ox * Math.sin(vz)) + (ty * Math.cos(vz));\n\n\t\t\tsbv.balls[i].position.x = (512 * tx) / (d - tz) + w / 2;\n\t\t\tsbv.balls[i].position.y = (h/2) - (512 * ty) / (d - tz);\n\n\t\t}\n\n\t\trenderer.render(stage);\n\n\t\trequestAnimFrame(update);\n\t}\n"
